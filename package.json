{
  "name": "sheet-cmd",
  "version": "1.0.3",
  "packageManager": "npm@10.9.0",
  "description": "A CLI tool to interact with Google Sheets - perfect for LLM integrations",
  "type": "module",
  "main": "dist/cli.js",
  "bin": {
    "sheet-cmd": "./dist/cli.js",
    "sheet": "./dist/cli.js"
  },
  "scripts": {
    "dev": "tsx src/cli.ts",
    "typecheck": "tsc --noEmit",
    "build": "rimraf ./dist && tsc",
    "start": "node dist/cli.js",
    "test": "vitest --config tests/unit/vitest.config.ts",
    "test:unit": "vitest run --config tests/unit/vitest.config.ts",
    "changeset": "changeset",
    "release": "npm run build && changeset publish",
    "turbo:typecheck": "turbo typecheck",
    "turbo:build": "turbo build",
    "turbo:dev": "turbo dev",
    "turbo:test:unit": "turbo test:unit"
  },
  "keywords": [
    "google-sheets",
    "llm",
    "tool",
    "cli"
  ],
  "author": "Lucas Vieira",
  "license": "MIT",
  "homepage": "https://github.com/lucasvtiradentes/sheet-cmd#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/lucasvtiradentes/sheet-cmd.git"
  },
  "bugs": {
    "url": "https://github.com/lucasvtiradentes/sheet-cmd/issues"
  },
  "dependencies": {
    "chalk": "^5.4.1",
    "commander": "^14.0.0",
    "google-auth-library": "^9.14.2",
    "google-spreadsheet": "^4.1.4",
    "inquirer": "^12.7.0",
    "zod": "^4.0.5"
  },
  "devDependencies": {
    "@changesets/cli": "^2.29.7",
    "@types/inquirer": "^9.0.8",
    "@types/node": "^22.10.5",
    "rimraf": "^5.0.5",
    "tsx": "^4.19.2",
    "turbo": "^2.5.8",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  }
}
